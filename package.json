{
  "name": "@bluejay/access-control",
  "version": "1.0.0",
  "description": "Simple, flexible and reliable access control for NodeJS and Typescript.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --require ./node_modules/ts-node/register --require ./test/setup ./test/**/**.test.js",
    "docs": "./node_modules/.bin/typedoc --out docs --mode file --excludeExternals --excludePrivate ./index.ts",
    "prepare": "rm -rf dts && tsc && tsc --outDir dts -d"
  },
