  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/chai-subset": "^1.3.1",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "chai-subset": "^1.6.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "ts-node": "^3.3.0",
    "typedoc": "^0.9.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.84",
    "lodash": "^4.17.4"
  }
}
